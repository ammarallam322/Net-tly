@model teamProject.viewModel.ClientViewModel

@{
    ViewData["Title"] = "Delete";
}

<h3>Are you sure you want to delete this?</h3>
<div>
    
    <hr />
    <dl class="row">
        
        <dt class="col-sm-2">
            Name
        </dt>
        <dd class="col-sm-10">
            @Model.Name
        </dd>
        <dt class="col-sm-2">
            Email
        </dt>
        <dd class="col-sm-10">
            @Model.Email
        </dd>
        <dt class="col-sm-2">
            SSN
        </dt>
        <dd class="col-sm-10">
            @Model.SSN
        </dd>
  
       
    </dl>

    <form asp-action="Delete">
        <input type="submit" value="Delete" class="btn btn-danger" /> |
        <a href="@Url.Action("Index")">Back to List</a>
    </form>
</div>
